<div class="right-blogs col-md-10">
	<% PostGet_all.forEach(	function(PostGet_all){%>
	<article class="right-blogs-box clearfix">
		<div class="right-blogs-frame col-md-10">
			<h2 class="right-blogs-title">
				<a href="/blogsById?blogs_ids=<%= PostGet_all.kt_blogs_ids %>&tags_ids=<%= PostGet_all.kt_tags_ids %>" title="<%= PostGet_all.kt_blogs_titles %>"  target="_blank" ><%= PostGet_all.kt_blogs_titles %></a>
			</h2>
			<p><%- PostGet_all.kt_blogs_contents.substring(0,100) %>...</p>
		</div>
		<div class="right-blogs-bar col-md-2" >
			<div class="right-bar-date">
				<strong class="right-bar-day"><%= PostGet_all.kt_blogs_ri %></strong>
				<div class="right-bar-holder">
					<span class="right-bar-month"><%= PostGet_all.kt_blogs_yue %></span>
					<span class="right-bar-year"><%= PostGet_all.kt_blogs_year %></span>
				</div>
			</div>
			<div class="right-bar-tag">
				<strong class="right-bar-title">标签</strong>
				<a href="/blogsByMenu?blogs_ids=<%= PostGet_all.kt_blogs_ids %>&tags_ids=<%= PostGet_all.kt_tags_ids %>" title="#" rel="external"><%= PostGet_all.kt_tags_name %></a>
			</div>
		</div>
	</article>
	<% });%>
	<div id="pagination">
		<%- Create_links %>
	</div>
	<div style="clear:both;"></div>
</div>