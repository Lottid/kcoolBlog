<!-- 开始循环 -->
<% PostDataProse.forEach(	function(PostDataProse){%>
<div class="prose-tree col-md-12">
	<h2><a href="#"><%= PostDataProse %>年<i></i></a></h2>
	<ul class="list">
	<% PostGetProse.forEach(	function(PostGetProse){%>
		<% if( PostDataProse ==  PostGetProse.dataYear){%>
		<li class="cls">
			<p class="date"><%= PostGetProse.dataMonth %>月</p>
			<p class="intro"><%= PostGetProse.kt_prose_titles %></p>
			<p class="version"><%= PostGetProse.kt_prose_dates %></p>
			<div class="more">
				<p><%- PostGetProse.kt_prose_contents %></p>
			</div>
		</li>
		<%  } %>
	<% });%>
	<!-- 开始循环结束 -->
	</ul>
</div>
<% });%>